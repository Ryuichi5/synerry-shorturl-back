{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/synerry-shorturl-back/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/synerry-shorturl-back/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/synerry-shorturl-back",
  "projects": {
    "synerry-shorturl-back": {
      "type": "application",
      "root": "apps/synerry-shorturl-back",
      "entryFile": "main",
      "sourceRoot": "apps/synerry-shorturl-back/src",
      "compilerOptions": {
        "tsConfigPath": "apps/synerry-shorturl-back/tsconfig.app.json"
      }
    },
    "my-app": {
      "type": "application",
      "root": "apps/my-app",
      "entryFile": "main",
      "sourceRoot": "apps/my-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/my-app/tsconfig.app.json"
      }
    },
    "test-server": {
      "type": "application",
      "root": "apps/test-server",
      "entryFile": "main",
      "sourceRoot": "apps/test-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/test-server/tsconfig.app.json"
      }
    }
  }
}